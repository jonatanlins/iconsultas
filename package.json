{
  "name": "iconsultas",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "react-transition-group": "^2.5.3",
    "styled-components": "^4.1.3",
    "tiny-slider-react": "^0.3.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "precommit": "lint-staged",
    "format": "prettier --config package.json --write \"src/**/*.{js,jsx,json,scss,sass,html,css}\""
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,scss,sass,html,css}": [
      "prettier --config package.json --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-plugin-prettier": "^3.0.1",
    "gh-pages": "^2.0.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0"
  }
}